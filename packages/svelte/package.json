{
	"name": "@git-stats-components/svelte",
	"version": "1.0.6",
	"description": "Beautiful GitHub/GitLab/Bitbucket contribution graphs for Svelte",
	"author": "Derek Johnston",
	"license": "MIT",
	"type": "module",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./ContributionGraph.svelte": "./src/ContributionGraph.svelte",
		"./StatsBreakdown.svelte": "./src/StatsBreakdown.svelte"
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE",
		"screenshot-*.png"
	],
	"keywords": [
		"svelte",
		"github",
		"gitlab",
		"bitbucket",
		"contributions",
		"stats",
		"component"
	],
	"scripts": {
		"build": "svelte-package -i src -o dist",
		"dev": "svelte-package -i src -o dist --watch",
		"clean": "rm -rf dist",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui"
	},
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0"
	},
	"dependencies": {
		"@git-stats-components/core": "^1.0.0"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.2.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tsconfig/svelte": "^5.0.6",
		"@vitest/ui": "^1.0.4",
		"jsdom": "^23.0.1",
		"svelte": "^4.2.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.0",
		"vitest": "^1.0.4"
	}
}
